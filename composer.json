{
    "name": "nzolt/linvest",
    "type": "project",
    "description": "Calculate investment interest rate.",
    "keywords": [
        "Test project Landinvest",
        "no framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "league/climate": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "phpunit/php-code-coverage": "^9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
